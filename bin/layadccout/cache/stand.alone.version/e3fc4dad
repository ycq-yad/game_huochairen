          var newMatrix = tResultMatrix.clone();
          tResultMatrix = newMatrix, this._preGraphicMatrix = tResultMatrix;
        }
        return tResultMatrix;
      }
    }, {
      key: "draw",
      value: function draw(graphics, boneMatrixArray) {
