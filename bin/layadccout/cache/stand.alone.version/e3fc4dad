      }
    }, {
      key: "skew",
      value: function skew(m, x, y) {
        var sinX = Math.sin(y),
            cosX = Math.cos(y),
            sinY = Math.sin(x),
            cosY = Math.cos(x);
        return m.setTo(m.a * cosY - m.b * sinX, m.a * sinY + m.b * cosX, m.c * cosY - m.d * sinX, m.c * sinY + m.d * cosX, m.tx * cosY - m.ty * sinX, m.tx * sinY + m.ty * cosX), m;
      }
    }]);

    return Transform;
  }();

