        }

        if (percentPosition && (position *= pathLength), percentSpacing) for (i = 0; i < spacesCount; i++) {
          spaces[i] *= pathLength;
        }
        var segment,
            segments = this._segments,
            curveLength = 0;

        for (i = 0, o = 0, curve = 0, segment = 0; i < spacesCount; i++, o += 3) {
